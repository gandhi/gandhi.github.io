{
    "sass.enabled": false,
    "path": {
        "scss/main.scss": {
            "sass.enabled": true,
            "sass.options": {
                "includePaths": [
                    "../bower_components/normalize-scss/"
                ],
                "outputDir": "../css",
                "imagePath": null,
                "sourceComments": false,
                "outputStyle": "nested"
            },
            "linting.collapsed": true
        }
    },
    "themes.theme": "rain.monokai-dark-soda",
    "useTabChar": false,
    "spaceUnits": 2,
    "brackets-git.gitPath": "git"
}